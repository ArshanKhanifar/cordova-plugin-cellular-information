<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        id="cordova-plugin-cellular-information" version="0.2.3">
    <name>Cell Info</name>
    <description>Show's phone's cellular info</description>
    <license>Apache 2.0</license>
    <keywords>cordova,sim,carrier,mcc,mnc,telephonenumber</keywords>
    <js-module src="www/cellularInfo.js" name="CellularInformation">
        <clobbers target="CellularInformation" />
    </js-module>
    <repo>https://github.com/arshankhanifar/cordova-plugin-cellular-information</repo>

      <!-- ios -->
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
        <feature name="CellularInformation">
            <param name="ios-package" value="CellularInformation" />
        </feature>
        </config-file>
        <header-file src="src/ios/CellularInformation.h"/>
        <source-file src="src/ios/CellularInformation.m"/>
        <framework src="CoreTelephony.framework"/>
    </platform>

</plugin>